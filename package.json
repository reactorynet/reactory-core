    "prop-types": "15.8.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-i18next": "14.1.2",
    "react-router": "6.24.1",
    "react-router-dom": "6.24.1",
    "rollup": "4.18.1",
    "rollup-copy-plugin": "0.1.0",
    "rollup-plugin-jsx": "1.0.3",
    "shelljs": "0.8.5",
    "ts-jest": "29.2.2",
    "ts-node": "10.9.2",
    "typescript": "5.5.3",
    "typescript-eslint": "7.16.0"
  },
  "nodemonConfig": {
    "ignore": [
      "test/*",
      "docs/*",
      ".git",
      "node_modules/**/node_modules"
    ],
    "delay": "3500",
    "verbose": true
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": [
      "airbnb/base",
      "plugin:import/errors"
    ],
    "rules": {
      "no-use-before-define": 0,
      "arrow-body-style": 0,
      "dot-notation": 0,
      "no-console": 0,
      "no-underscore-dangle": 0,
      "linebreak-style": 1
    },
    "env": {
      "mocha": true
    }
  }
}