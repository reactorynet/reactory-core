{
  "name": "@reactory/server-core",
  "version": "0.9.47",
  "displayName": "Reactory Server",
  "description": "Reactory Core Server",
  "main": "server.js",
  "module": "reactory-api",
  "private": true,
  "author": {
    "name": "Werner Weber",
    "email": "werner.weber@reactory.net"
  },
  "licenses": [
    {
      "license": "Apache-2.0"
    }
  ],
  "moduleNameMapper": {
    "^reactory/server-core(.*)$": "<rootDir>/src/"
  },
  "scripts": {
    "rollup": "rollup --config rollup.config.dev.js",
    "build-serve": "npm-run-all clean build serve",
    "make-install": "npm install & npm run build-serve",
    "test": "mocha **/*.spec.js",
    "reactory:create-plugin": "NODE_PATH=./src npx babel-node ./src/scripts/create-plugin.js --presets @babel/env --max_old_space_size=200000",
    "reactory:create-form": "NODE_PATH=./src npx babel-node ./src/scripts/create-form.js --presets @babel/env --max_old_space_size=200000",
    "lint": "eslint . --fix"
  },
  "repository": {
    "type": "git",
    "url": "https://bitbucket.org/WernerWeber/reactory-server.git"
  },
  "dependencies": {
    "@babel/cli": "7.4.4",
    "@babel/core": "7.4.4",
    "@babel/eslint-plugin-development": "1.0.1",
    "@babel/node": "7.4.5",
    "@babel/plugin-proposal-class-properties": "7.4.0",
    "@babel/plugin-proposal-decorators": "7.4.4",
    "@babel/plugin-proposal-do-expressions": "7.2.0",
    "@babel/plugin-proposal-export-default-from": "^7.2.0",
    "@babel/plugin-proposal-export-namespace-from": "7.2.0",
    "@babel/plugin-proposal-function-bind": "7.2.0",
    "@babel/plugin-proposal-function-sent": "7.2.0",
    "@babel/plugin-proposal-json-strings": "7.2.0",
    "@babel/plugin-proposal-logical-assignment-operators": "7.2.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "7.4.4",
    "@babel/plugin-proposal-numeric-separator": "7.2.0",
    "@babel/plugin-proposal-optional-chaining": "7.2.0",
    "@babel/plugin-proposal-pipeline-operator": "7.3.2",
    "@babel/plugin-proposal-throw-expressions": "7.2.0",
    "@babel/plugin-syntax-class-properties": "7.2.0",
    "@babel/plugin-syntax-dynamic-import": "7.2.0",
    "@babel/plugin-syntax-import-meta": "7.2.0",
    "@babel/plugin-transform-arrow-functions": "^7.2.0",
    "@babel/plugin-transform-classes": "7.4.4",
    "@babel/plugin-transform-modules-commonjs": "7.4.3",
    "@babel/plugin-transform-object-set-prototype-of-to-assign": "7.2.0",
    "@babel/plugin-transform-react-constant-elements": "^7.2.0",
    "@babel/plugin-transform-react-inline-elements": "7.2.0",
    "@babel/plugin-transform-react-jsx": "7.3.0",
    "@babel/plugin-transform-react-jsx-compat": "^7.2.0",
    "@babel/plugin-transform-runtime": "7.4.4",
    "@babel/plugin-transform-typescript": "^7.6.3",
    "@babel/preset-env": "7.4.4",
    "@babel/preset-react": "7.0.0",
    "@babel/preset-typescript": "^7.6.0",
    "@babel/runtime": "7.4.3",
    "core-js": "^3.18.3",
    "apollo-link-context": "1.0.19",
    "apollo-link-ws": "^1.0.19",
    "apollo-server-express": "^2.11.0",
    "archiver": "^3.0.0",
    "babel-eslint": "10.0.1",
    "babel-loader": "8.0.6",
    "babel-plugin-add-module-exports": "1.0.2",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-plugin-named-asset-import": "0.3.2",
    "babel-preset-react-app": "9.0.0",
    "babel-runtime": "^6.26.0",
    "body-parser": "^1.19.0",
    "chart.js": "^2.8.0",
    "chartjs-node": "^1.7.1",
    "chartjs-node-canvas": "^2.4.0",
    "chartjs-plugin-datalabels": "^0.7.0",
    "eslint": "^4.8.0",
    "eslint-config-airbnb": "^16.0.0",
    "eslint-plugin-react": "^7.4.0",
    "mocha": "^8.0.1",
    "moment": "^2.24.0",
    "mongodb": "^3.2.7",
    "mongoose": "^5.6.0",
    "object-mapper": "^5.0.0",
    "rollup": "^1.16.2",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-node-resolve": "^5.0.4",
    "schema-inspector": "^1.6.9",
    "sha1": "^1.1.1"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^8.3.0",
    "@types/express": "^4.17.2",
    "@types/express-session": "^1.15.8",
    "@types/lodash": "^4.14.149",
    "@types/mocha": "^7.0.2",
    "@types/mongodb": "^3.3.11",
    "@types/mongoose": "^5.5.32",
    "@types/mysql": "^2.15.8",
    "@types/node": "10.17.49",
    "@types/passport-http": "^0.3.6",
    "@types/passport-jwt": "3.0.1",
    "@types/sha1": "^1.1.2",
    "@types/uuid": "^3.4.6",
    "proxyquire": "^2.1.3",
    "rollup-plugin-jsx": "^1.0.3",
    "rollup-plugin-replace": "^2.2.0",
    "sinon": "^9.0.2",
    "ts-node": "^8.10.2",
    "typescript": "3.4.3"
  },
  "nodemonConfig": {
    "ignore": [
      "test/*",
      "docs/*",
      ".git",
      "node_modules/**/node_modules"
    ],
    "delay": "3500",
    "verbose": true
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": [
      "airbnb/base",
      "plugin:import/errors"
    ],
    "rules": {
      "no-use-before-define": 0,
      "arrow-body-style": 0,
      "dot-notation": 0,
      "no-console": 0,
      "no-underscore-dangle": 0,
      "linebreak-style": 1
    },
    "env": {
      "mocha": true
    }
  }
}
