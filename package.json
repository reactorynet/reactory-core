    "d3-color": "3.1.0",
    "d3-force": "3.0.0",
    "d3-delaunay": "6.0.4",
    "eslint": "9.7.0",
    "eventemitter3": "5.0.1",
    "exceljs": "4.4.0",
    "graphql": "16.9.0",
    "i18next": "23.11.5",
    "inversify": "6.0.2",
    "jest": "29.7.0",
    "jsdoc": "4.0.3",
    "jsdoc-ts-utils": "5.0.0",
