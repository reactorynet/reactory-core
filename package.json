{
  "name": "@reactory/reactory-core",
  "version": "1.0.3",
  "displayName": "Reactory Core",
  "description": "Reactory Core - shared across all platforms and services. Contains type definitions, constants and shared components for the reactory eco-system.",
  "main": "dist/reactory.core.js",
  "module": "reactory-core",
  "typings": "dist/types/index.d.ts",
  "files": [
    "dist",
    "docs",
    "branding"
  ],
  "type": "commonjs",
  "author": {
    "name": "Werner Weber",
    "email": "werner.weber@reactory.net"
  },
  "licenses": [
    {
      "license": "Apache-2.0"
    }
  ],
  "moduleNameMapper": {
    "^reactory/reactory-core(.*)$": "<rootDir>/src/"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "rollup": "rollup --config rollup.config.js",
    "package": "npm pack --pack-destination $REACTORY_PLUGINS/artifacts",
    "build:install": "npm-run-all clean rollup docs package deploy:local",
    "deploy:local": "NODE_PATH=./scripts npx babel-node ./scripts/deploy.js --presets @babel/env",  
    "test": "jest",
    "lint": "eslint . --fix",
    "docs": "jsdoc -c jsdoc.json"
  },
  "repository": {
    "type": "git",
    "url": "https://bitbucket.org/reactory/reactory-core.git"
  },
  "dependencies": {
    "@apollo/client": "3.5.9",
    "reflect-annotations": "4.0.0-alpha.9",
    "reflect-args": "2.0.0",
    "reflect-metadata": "0.1.13"
  },
  "peerDependencies": {
    "react": "17.0.2"
  },
  "devDependencies": {
    "@babel/cli": "7.20.7",
    "@babel/core": "7.20.12",
    "@babel/eslint-plugin-development": "7.19.1",
    "@babel/node": "7.20.7",
    "@babel/plugin-proposal-class-properties": "7.18.6",
    "@babel/preset-env": "7.20.2",
    "@babel/preset-react": "7.18.6",
    "@babel/preset-typescript": "7.18.6",
    "@babel/register": "7.18.9",
    "@babel/types": "7.20.7",
    "@emotion/core": "11.0.0",
    "@emotion/styled": "11.10.5",
    "@mui/icons-material": "5.15.15",
    "@mui/lab": "5.0.0-alpha.170",
    "@mui/material": "5.15.15",
    "@mui/styles": "5.15.15",
    "@mui/types": "7.2.14",
    "@rollup/plugin-babel": "6.0.3",
    "@rollup/plugin-commonjs": "21.0.1",
    "@rollup/plugin-node-resolve": "13.1.3",
    "@rollup/plugin-replace": "3.1.0",
    "@rollup/plugin-typescript": "^8.3.0",
    "@types/d3": "^7.4.3",
    "@types/d3-array": "3.2.1",
    "@types/d3-cloud": "1.2.0",
    "@types/d3-color": "3.0.0",
    "@types/d3-force": "3.0.0",
    "@types/d3-delaunay": "6.0.0",
    "@types/classnames": "^2.3.1",
    "@types/human-date": "^1.4.2",
    "@types/human-number": "^1.0.0",
    "@types/jest": "^29.4.0",
    "@types/localforage": "^0.0.34",
    "@types/lodash": "4.14.178",
    "@types/mocha": "9.1.0",
    "@types/mongodb": "^3.3.11",
    "@types/mongoose": "5.11.97",
    "@types/node": "12.20.46",
    "@types/object-mapper": "^6.2.0",
    "@types/postal": "1.0.31",
    "@types/passport-http": "^0.3.6",
    "@types/passport-jwt": "3.0.1",
    "@types/react": "17.0.2",
    "@types/sha1": "^1.1.2",
    "@types/shelljs": "^0.8.11",
    "@types/uuid": "^3.4.6",
    "@typescript-eslint/eslint-plugin": "^5.59.6",
    "@typescript-eslint/parser": "^5.59.6",
    "babel-eslint": "^10.1.0",
    "better-docs": "^2.7.2",
    "chai": "4.3.6",
    "chart.js": "3.7.0",
    "chartjs-node-canvas": "4.1.6",
    "classnames": "^2.3.1",
    "d3": "^6.7.0",
    "d3-3d": "1.0.0",
    "d3-array": "3.2.4",
    "d3-cloud": "1.2.7",
    "d3-color": "3.1.0",
    "d3-force": "3.0.0",
    "d3-delaunay": "6.0.4",    
    "eslint": "^8.40.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "7.28.0",
    "eventemitter3": "^3.1.0",
    "exceljs": "4.3.0",
    "i18next": "^21.6.5",
    "inversify": "^6.0.1",
    "jest": "^29.5.0",
    "jsdoc": "^4.0.2",
    "jsdoc-ts-utils": "^3.1.0",
    "localforage": "1.10.0",
    "moment": "^2.29.4",
    "mongodb": "4.5.0",
    "mongoose": "6.8.3",
    "npm-run-all": "4.1.5",
    "object-mapper": "6.2.0",
    "postal": "2.0.6",
    "prettier": "^2.8.8",
    "react": "17.0.2  ",
    "react-dom": "17.0.2",
    "react-i18next": "^11.15.3",
    "react-router": "6.3.0",
    "react-router-dom": "6.3.0",
    "rollup": "2.75.6",
    "rollup-copy-plugin": "^0.1.0",
    "rollup-plugin-jsx": "1.0.3",
    "shelljs": "0.8.5",
    "ts-jest": "^29.0.5",
    "ts-node": "10.7.0",
    "typescript": "4.5.5"
  },
  "nodemonConfig": {
    "ignore": [
      "test/*",
      "docs/*",
      ".git",
      "node_modules/**/node_modules"
    ],
    "delay": "3500",
    "verbose": true
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": [
      "airbnb/base",
      "plugin:import/errors"
    ],
    "rules": {
      "no-use-before-define": 0,
      "arrow-body-style": 0,
      "dot-notation": 0,
      "no-console": 0,
      "no-underscore-dangle": 0,
      "linebreak-style": 1
    },
    "env": {
      "mocha": true
    }
  }
}
